{
  "name": "yusronarif/laravel-core",
  "description": "Laravel Custom Core",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Much Yusron Arif",
      "email": "yusron.arif4@gmail.com",
      "homepage": "https://yusronarif.github.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "nesbot/carbon": ">=2.0"
  },
  "require-dev": {
    "laravel/framework": ">=5.5",
    "phpunit/phpunit": ">=6.0"
  },
  "autoload": {
    "psr-4": {
      "Yusronarif\\Core\\": "src/"
    },
    "files": [
      "helpers/helpers.php",
      "helpers/html_elements.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Yusronarif\\Core\\Providers\\DbServiceProvider",
        "Yusronarif\\Core\\Providers\\BladeServiceProvider"
      ],
      "aliases": {}
    }
  }
}
